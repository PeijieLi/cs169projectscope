
<div class="col-md-2"></div>
<div class="col-md-10">
  <table class="table table-bordered">
    <tbody>
    <%- @rubrics.each_with_index do |r, i| %>
        <%- r_params = JSON.parse(r.params) %>
        <%- score = @metric_samples[i].nil? ? -1 : @metric_samples[i].score %>
        <tr>
          <th>
            <%= r.metric_name.humanize %>
          </th>
          <td bgcolor=<%=bgcolor_for_score(score, r_params['max'])%>>
            <%= score %>
          </td>
          <td>
            <%= r_params['max'] %>
          </td>
        </tr>
    <%- end %>
    </tbody>
  </table>
</div>
<div class="col-md-2"></div>
